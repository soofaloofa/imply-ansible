druid.service=broker
druid.port=8082

# HTTP server threads
druid.broker.http.numConnections={{druid.broker.http.num_connections}}
druid.server.http.numThreads={{druid.server.http.num_threads}}

# Processing threads and buffers
druid.processing.buffer.sizeBytes={{druid.processing.buffer.size_bytes}}
druid.processing.numThreads={{druid.processing.num_threads}}

# Query cache
druid.broker.cache.useCache=true
druid.broker.cache.populateCache=true
druid.cache.type=local
druid.cache.sizeInBytes={{druid.cache.size_in_bytes}}
